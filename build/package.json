{
  "name": "electron-easy-start",
  "version": "1.0.0",
  "description": "MailTool",
  "main": "electron_process.js",
  "homepage": "./",
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-server": "nodemon ./server/bin/www",
    "electron-dev": "ELECTRON_START_URL=http://localhost:4000 electron .",
    "test": "react-scripts-ts test --env=jsdom",
    "build-web": "npm-run-all build-css build-js",
    "build-desktop": "npm-run-all build-web gulp dist",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "build-js": "react-scripts-ts build",
    "electron": "electron ./build",
    "dist": "build",
    "gulp": "gulp build",
    "start-electron": "npm-run-all -p start electron-dev",
    "package-win": "electron-packager . myApp --platform=win32 --arch=x64 --version=0.33.3 --app-version=0.1.0 --out=dist --ignore=dist --prune"
  },
  "build": {
    "appId": "electroneasystart",
    "files": [
      {
        "from": "./build/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      }
    ],
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "author": "Roman Turusov <mr.romatur@gmail.com>",
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.0",
    "@types/react": "^16.0.41",
    "@types/react-dom": "^16.0.4",
    "@types/react-modal": "^3.1.2",
    "@types/react-router-dom": "^4.2.6",
    "electron": "^1.8.2",
    "electron-builder": "^20.8.1",
    "electron-reload": "^1.2.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react-scripts-ts": "2.14.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@types/gsap": "^1.19.2",
    "cookie-parser": "~1.4.3",
    "debug": "~3.1.0",
    "express": "~4.16.0",
    "gsap": "^1.20.4",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mailgun-js": "^0.16.0",
    "morgan": "~1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-modal": "^3.3.2",
    "react-router-dom": "^4.2.2",
    "react-router-page-transition": "^3.1.0"
  }
}
